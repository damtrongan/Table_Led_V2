<%- include('./head.ejs') %>

    <div class="body">
      <table id="table2" class="table-refresh">
        <tr class="name_station">
          <th>Parameter</th>
          <th>Station 1</th>
          <th>Station 2</th>
          <th>Unit</th>
        </tr>
        <tr class="parameter_1">
          <th>pH</th>
          <td>
            <%= data.station0.pH.value %>
          </td>
          <td>
            <%= data.station1.pH.value %>
          </td>
          <td>
            <%= data.station0.pH.unit %>
          </td>
        </tr>
        <tr class="parameter_2">
          <th>Temp</th>
          <td>
            <%= data.station0.Temp.value %>
          </td>
          <td>
            <%= data.station1.Temp.value %>
          </td>
          <td>*C</td>
        </tr>
        <tr class="parameter_3">
          <th>TSS</th>
          <td>50</td>
          <td>60</td>
          <td>mg/L</td>
        </tr>
        <tr class="parameter_4">
          <th>COD</th>
          <td>50</td>
          <td>60</td>
          <td>mg/L</td>
        </tr>
        <tr class="parameter_5">
          <th>NH4</th>
          <td>2</td>
          <td>6</td>
          <td>mg/L</td>
        </tr>
        <tr class="parameter_6">
          <th>Flow IN</th>
          <td>50</td>
          <td>60</td>
          <td>m3/h</td>
        </tr>
      </table>
    </div>
  </div>
  <script src="main.js"></script>
</body>

</html>